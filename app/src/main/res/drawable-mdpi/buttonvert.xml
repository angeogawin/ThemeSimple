<?xml version="1.0" encoding="utf-8" ?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true" >
        <shape>
            <solid
                android:color="#eae6e6" />
            <stroke
                android:width="1dp"
                android:color="#eae6e6" />
            <corners
                android:radius="5dp" />
            <padding
                android:left="2dp"
                android:top="2dp"
                android:right="2dp"
                android:bottom="2dp" />
        </shape>
    </item>
    <item>
        <shape>
            <solid
                android:color="#009e0f" />

            <stroke
                android:width="1dp"
                android:color="#009e0f" />
            <corners
                android:radius="5dp" />
            <padding
                android:left="10dp"
                android:top="10dp"
                android:right="10dp"
                android:bottom="10dp" />
        </shape>
    </item>
</selector>